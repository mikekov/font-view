{
	"name": "font-view",
	"productName": "Font Viewer",
	"version": "0.1.0",
	"main": "main.js",
	"scripts": {
		"ng": "ng",
		"prestart": "npm run svg",
		"start": "npm run serve",
		"serve": "ng serve --port=4444",
		"prebuild": "npm run svg",
		"build": "ng build --prod --base-href ./",
		"postbuild": "cpy package-app.json ./dist --rename=package.json && cpy main.js ./dist",
		"dbg": "electron --remote-debugging-port=9222 --no-first-run --no-default-browser-check ./main_dbg.js",
		"go": "/home/mike/dev/font-view/node_modules/.bin/electron '--remote-debugging-port=9222' '--no-first-run' '--no-default-browser-check' './main_dbg.js'",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"svg": "svg-sprite --config sprite-config.json icons/*.svg",
		"pack:linux": "electron-packager ./dist font-view --overwrite --platform=linux --arch=x64 --icon=icon.png --prune=true --out=release-builds",
		"pack:mac": "electron-packager ./dist --overwrite --platform=darwin --arch=x64 --icon=icon.icns --prune=true --out=release-builds",
		"pack:win": "electron-packager ./dist font-view --overwrite --asar --platform=win32 --arch=ia32 --icon=icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Font Viewer\""
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~10.0.1",
		"@angular/cdk": "^10.0.0",
		"@angular/common": "~10.0.1",
		"@angular/compiler": "~10.0.1",
		"@angular/core": "~10.0.1",
		"@angular/forms": "~10.0.1",
		"@angular/material": "^10.0.0",
		"@angular/platform-browser": "~10.0.1",
		"@angular/platform-browser-dynamic": "~10.0.1",
		"@angular/router": "~10.0.1",
		"lodash": "^4.17.15",
		"opentype.js": "^1.3.3",
		"rxjs": "~6.5.4",
		"tslib": "^2.0.0",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "^9.2.0",
		"@angular-devkit/build-angular": "~0.1000.0",
		"@angular/cli": "~10.0.0",
		"@angular/compiler-cli": "~10.0.1",
		"@types/jasmine": "~3.5.0",
		"@types/jasminewd2": "~2.0.3",
		"@types/lodash": "^4.14.154",
		"@types/node": "^12.12.43",
		"@types/opentype.js": "^0.7.2",
		"codelyzer": "^5.1.2",
		"cpy-cli": "^3.1.1",
		"electron": "^9.0.5",
		"electron-packager": "^15.0.0",
		"jasmine-core": "~3.5.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~6.3.16",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.2",
		"karma-jasmine": "~3.3.0",
		"karma-jasmine-html-reporter": "^1.5.0",
		"protractor": "~7.0.0",
		"svg-sprite": "^1.5.0",
		"ts-node": "~8.3.0",
		"tslint": "~6.1.0",
		"typescript": "~3.9.5"
	}
}
